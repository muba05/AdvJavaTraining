    </servlet>

    <servlet-mapping>
        <servlet-name>FlightBooking</servlet-name>
        <url-pattern>/FlightBook</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>FlightBooking1</servlet-name>
